{
  "name": "swp/output-channel-bundle",
  "type": "symfony-bundle",
  "description": "Output Channel implementation for Symfony applications.",
  "keywords": ["channel", "output", "superdesk", "publisher", "publishing"],
  "license":"AGPL-3.0",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Rafał Muszyński",
      "email": "rafal.muszynski@sourcefabric.org"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../../*/*"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/dependency-injection": "^3.4|^4.2",
    "symfony/config": "^3.4|^4.2",
    "symfony/form": "^3.4|^4.2",
    "swp/storage-bundle": "^1.4|^2.0",
    "swp/output-channel": "^1.4|^2.0",
    "swp/common": "^1.4|^2.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.0"
  },
  "autoload": {
    "exclude-from-classmap": [
      "spec/"
    ],
    "psr-4": { "SWP\\Bundle\\OutputChannel\\": "" }
  },
  "autoload-dev": {
    "psr-4": {
      "SWP\\Bundle\\OutputChannel\\spec\\": "spec/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
